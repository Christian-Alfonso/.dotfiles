{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{ if .Root }}root @ {{ end }}{{ .Shell }} in {{ .Folder }}",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#03DED3",
          "background": "transparent",
          "style": "diamond",
          "template": "{{.Icon}} {{ if .WSL }}(WSL){{ else }}{{ end }} ",
          "type": "os"
        },
        {
          "template": "{{ .UserName }}@{{ .HostName }}",
          "foreground": "#03DED3",
          "background": "transparent",
          "type": "session",
          "style": "diamond",
          "properties": {
            "cache_duration": "none"
          }
        },
        {
          "template": "\uf0e7 ",
          "foreground": "#ffff66",
          "background": "transparent",
          "type": "root",
          "style": "diamond",
          "properties": {
            "cache_duration": "none"
          }
        },
        {
          "background": "transparent",
          "foreground": "#D6DEEB",
          "style": "diamond",
          "template": " \uf09c admin ",
          "type": "root"
        },
        {
          "foreground": "#E8EAEE",
          "background": "transparent",
          "type": "cmake",
          "style": "diamond",
          "template": " \ue61e \ue61d cmake {{ .Full }} "
        },
        {
          "type": "python",
          "style": "diamond",
          "properties": {
            "display_mode": "context"
          },
          "foreground": "#011627",
          "background": "#2f2f2f",
          "template": " \ue73c {{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }} "
        },
        {
          "type": "go",
          "style": "diamond",
          "foreground": "#65cfdc",
          "background": "#2f2f2f",
          "template": " \u202d\ue626 {{ .Full }} "
        },
        {
          "type": "rust",
          "style": "diamond",
          "foreground": "#ff964f",
          "background": "#2f2f2f",
          "template": " \ue7a8 {{ .Full }} "
        },
        {
          "foreground": "#03DED3",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#E44141{{ end }}"
          ],
          "background": "transparent",
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "template": " {{ if gt .Code 0 }}\uf00d {{ .String }}{{ else }}\uf00c {{ end }} ",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#d29226",
          "properties": {
            "cache_duration": "none",
            "folder_icon": "<#B5B2C2>\ue5fe</>",
            "style": "full"
          },
          "leading_diamond": "<#ffd04b>\uf07b</>",
          "style": "diamond",
          "template": "  {{ .Path }} ",
          "type": "path"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "branch_icon": "\uE0A0 ",
            "commit_icon": "\uF417 ",
            "tag_icon": "\uF412 ",
            "rebase_icon": "\uE728 ",
            "cherry_pick_icon": "\uE29B  ",
            "revert_icon": "\uF0E2  ",
            "merge_icon": "\uE727  "
          },
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}<#E84855> \uf044 {{ .Working.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#2FDA4E> \uf046 {{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "foreground": "#f9dd33",
          "type": "git",
          "style": "diamond",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#f9dd33{{ end }}",
            "{{ if gt .Ahead 0 }}#2EC4B6{{ end }}",
            "{{ if gt .Behind 0 }}#8A4FFF{{ end }}"
          ]
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#D6DEEB",
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\u276f",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "osc99": true,
  "final_space": true,
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#FEF5ED",
    "template": "$ "
  },
  "secondary_prompt": {
    "background": "transparent",
    "foreground": "#D6DEEB",
    "template": "\u2570\u2500\u276f "
  },
  "version": 3
}